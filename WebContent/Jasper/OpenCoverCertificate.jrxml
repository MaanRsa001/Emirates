	<subDataset name="Excess" uuid="97c19127-341e-41f8-9d9d-3753337bb1e3">
		<parameter name="Application_No" class="java.lang.String"/>
		<parameter name="branchCode" class="java.lang.String"/>
		<queryString>
			<![CDATA[select rownum sno,DESCRIPTION,to_char(SUM_INSURED,'999G999G999G999G999G999G999')SUM_INSURED,
trim(to_char(round(COMMODITY_EXCESS_PREMIUM,(select rfactor from currency_master cm
where  cm.CURRENCY_ID=mrd.CURRENCY_ID and rownum=1)),'999G999G999G999G990D99')) ||'  '||
(select CURRENCY_ABBREVIATION from branch_master cm where branch_code=$P{branchCode} and rownum=1) Excess
from marine_result_details mrd where application_no=$P{Application_No}]]>
		</queryString>
		<field name="SNO" class="java.math.BigDecimal"/>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="SUM_INSURED" class="java.lang.String"/>
		<field name="EXCESS" class="java.lang.String"/>
	</subDataset>
	<subDataset name="FreeText" uuid="672a15c4-6c35-48ef-b82b-9bec0444180c">
		<parameter name="Application_No" class="java.lang.String"/>
		<queryString>
			<![CDATA[Select Free_text_id,FREE_TEXT_DESCRIPTION from open_cover_free_text ocft,marine_data md
where proposal_no in (Select ocpm.proposal_no
from  open_cover_position_master ocpm,position_master pm
where ocpm.open_cover_no=pm.MISSIPPI_OPENCOVER_NO and pm.application_no=md.application_no
and ocpm.status='P')
and ocft.amend_id=(Select max(amend_id) from open_cover_free_text ocf
where ocft.proposal_no=ocf.proposal_no and md.cover_id=ocf.cover_id)
and md.cover_id=ocft.cover_id and md.application_no=$P{Application_No}]]>
		</queryString>
		<field name="FREE_TEXT_ID" class="java.math.BigDecimal"/>
		<field name="FREE_TEXT_DESCRIPTION" class="java.lang.String"/>
	</subDataset>
